result-view{
	match: card_struct (this)
	
	render{
		layout{
			section{
				content {
					for-each (this) {
						as (this) {
							image{
								url("#{value(this.card_pics.url)}")
                aspect-ratio(1:1)
                object-fit (Contain)
                background-color (Transparent)
                lightbox-enabled (false)
							}
						}
					}
				}
			}
		}
	}
}